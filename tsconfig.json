{
	"compilerOptions": {
		"esModuleInterop": true,
		"jsx": "react",
		"target": "ES2020",
		"moduleResolution": "Node",
		"module": "CommonJS",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"outDir": "./build",
		"baseUrl": "./backend/",
		"skipLibCheck": true,
		"strict": true
	},
	"exclude": ["src"]
}
